{
  "name": "lyef-redux-boilerplate",
  "version": "1.0.1",
  "description": "An opionated structure to create projects using React, Redux and Webpack.",
  "scripts": {
    "start": "npm run dev",
    "dev": "node -r babel-register ./node_modules/.bin/webpack-dev-server --config ./webpack/dev.js --watch --colors",
    "clean": "rimraf dist",
    "build": "npm run clean && NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack/prod.js --progress --profile --colors",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require tests/setup.js tests/specs/**/*.spec.js",
    "test:tdd": "npm test -- -w",
    "test:coverage": "NODE_ENV=test ./node_modules/.bin/nyc --reporter=html --reporter=text ./node_modules/.bin/mocha tests/setup.js tests/specs/**/*.spec.js",
    "lint": "./node_modules/.bin/eslint src",
    "lint:fix": "npm run lint -- --fix",
    "ci": "npm test && npm run lint",
    "prepush": "npm run ci"
  },
  "nyc": {
    "lines": 70,
    "check-coverage": true,
    "exclude": [
      "node_modules",
      "tests",
      "dist",
      "**/**/index.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lyef/lyef-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "boilerplate"
  ],
  "author": "lyef (http://lyef.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lyef/lyef-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/lyef/lyef-redux-boilerplate#README",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.4.5",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "3.0.0-beta.5",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-types": "^6.7.2",
    "chai": "^4.1.1",
    "chai-enzyme": "^0.8.0",
    "classnames": "^2.2.3",
    "copy-to-clipboard": "^3.0.4",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "es6-promise": "^4.1.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-import-resolver-babel-module": "4.0.0-beta.3",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.22.0",
    "husky": "^0.14.3",
    "jsdom": "^11.2.0",
    "mocha": "^4.0.1",
    "nyc": "^11.1.0",
    "postcss-discard-duplicates": "^2.0.1",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.5.3",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.5.5",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.3",
    "react-redux": "^5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-test-renderer": "16",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.0.0",
    "redux-thunk": "^2.1.0",
    "store": "^2.0.12"
  }
}
